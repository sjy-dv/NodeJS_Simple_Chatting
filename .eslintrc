{
    "plugins": ["prettier"],
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "script",
        "ecmaFeatures": {
            "jsx": false,
            "experimentalObjectRestSpread": true
        }
    },
    "env": {
        "browser": false,
        "node": true,
        "es6": true
    },
    "rules": {
        "prettier/prettier": ["error",{
            "endOfLine" : "auto"
        }],
        "no-unused-vars" : "off",
        "no-empty" : "off"
    }
}